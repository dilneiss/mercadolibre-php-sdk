{
    "name": "dilneiss/mercadolibre-php-sdk",
    "description": "This is a the codebase to generate a SDK for Open Platform Marketplace",
    "keywords": [
        "mercadolibre",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://developers.mercadolibre.com",
    "license": "unlicense",
    "authors": [
        {
            "name": "Mercado Libre",
            "homepage": "https://developers.mercadolibre.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "friendsofphp/php-cs-fixer": "*"
    },
    "autoload": {
        "psr-4": { "Meli\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Meli\\" : "test/" }
    }
}
